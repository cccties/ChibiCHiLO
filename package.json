    "@storybook/react": "^6.1.11",
    "@types/classnames": "^2.2.11",
    "@types/events": "^3.0.0",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/video.js": "^7.3.11",
    "@types/vimeo__player": "^2.10.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "@vimeo/player": "^2.14.1",
    "babel-loader": "^8.2.2",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "events": "^3.2.0",
    "glob": "^7.1.6",
    "iso-639-1": "^2.1.4",
    "jotai": "^0.12.2",
    "material-ui-confirm": "^2.1.1",
    "next": "^10.0.3",
    "npm-run-all": "^4.1.5",
    "outdent": "^0.7.1",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.13.1",
    "strict-event-emitter-types": "^2.0.0",
    "swr": "^0.3.9",
    "typescript": "^4.1.3",
    "video.js": "^7.10.2",
    "videojs-seek-buttons": "^1.6.0",
    "videojs-youtube": "^2.6.1"
  },
  "scripts": {
    "test": "yarn workspaces run test",
    "dev": "run-p dev:*",
    "dev:app": "next",
    "dev:server": "yarn --cwd server dev",
    "build": "run-s build:prisma build:next build:server",
    "build:prisma": "yarn --cwd server build:prisma",
    "build:next": "next build",
    "build:export": "next export -o server/public",
    "build:server": "yarn --cwd server build",
    "build:openapi": "openapi-generator-cli generate -i http://localhost:8080/api/v2/swagger/json -g typescript-fetch -o openapi",
    "start": "NODE_ENV=production node server/dist/index.js",
    "vercel-build": "yarn build && rm -rf public/api/v2/swagger && mkdir -p public/api/v2/swagger && cp -a server/dist/static public/api/v2/swagger/static",
    "format": "prettier --write .",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "lint:report": "yarn lint --output-file eslint_report.json --format json",
    "storybook": "start-storybook"
  },
  "engines": {
    "node": ">=12"
  }
}
